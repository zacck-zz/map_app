	</EditText>
	<TextView android:textColor="@android:color/black"
		android:textStyle="bold" android:text="Password" android:id="@+id/textView2"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
	<EditText android:layout_height="wrap_content" android:id="@+id/editText2"
		android:inputType="textPassword" android:layout_width="303dp"
		android:hint="Password ..."></EditText>
	<TextView android:textColor="@android:color/black"
		android:textStyle="bold" android:text="Confirm Password" android:id="@+id/textView7"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
	<EditText android:password="true" android:id="@+id/editText3"
