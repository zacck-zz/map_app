<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="@android:color/white"
  android:weightSum="1">
    <TableLayout android:id="@+id/tableLayout1" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <EditText android:id="@+id/editText1" android:layout_width="290dp" android:layout_height="wrap_content" android:hint="Search ...">
                <requestFocus></requestFocus>
            </EditText>
            <Button android:text="" android:id="@+id/button1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/search"></Button>
        </TableRow>
    </TableLayout>
    <TableLayout android:layout_width="match_parent" android:id="@+id/tableLayout3" android:layout_height="wrap_content" android:layout_weight="0.62">
        <TableRow android:id="@+id/tableRow7" android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="0dp">
            <Button android:layout_marginLeft="15dp" android:id="@+id/btnuserAccount" android:layout_height="wrap_content" android:text="Button" android:layout_weight="1" android:layout_width="0dp"></Button>
            <Button android:layout_marginRight="15dp" android:id="@+id/btnviewmap" android:layout_height="wrap_content" android:text="Button" android:layout_weight="1" android:layout_width="0dp"></Button>
        </TableRow>
        <TableRow android:id="@+id/tableRow8" android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="0dp">
            <TextView android:layout_marginLeft="35dp" android:textColor="@android:color/black" android:id="@+id/textView3" android:text="My Gitwork" android:layout_height="wrap_content" android:layout_width="0dp" android:layout_weight="1"></TextView>
            <TextView android:layout_marginLeft="15dp" android:textColor="@android:color/black" android:id="@+id/textView5" android:text="View Map" android:layout_height="wrap_content" android:layout_width="0dp" android:layout_weight="1"></TextView>
        </TableRow>
        <TableRow android:id="@+id/tableRow9" android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="0dp">
            <Button android:layout_marginLeft="15dp" android:id="@+id/btncategories" android:layout_height="wrap_content" android:text="Button" android:layout_weight="1" android:layout_width="0dp"></Button>
            <Button android:layout_marginRight="15dp" android:layout_height="wrap_content" android:text="Button" android:layout_weight="1" android:layout_width="0dp" android:id="@+id/btnusergroups"></Button>
        </TableRow>
        <TableRow android:id="@+id/tableRow10" android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="0dp">
            <TextView android:layout_marginLeft="35dp" android:textColor="@android:color/black"  android:id="@+id/textView6" android:text="Gitwork Categories" android:layout_weight="1" android:layout_height="wrap_content" android:layout_width="0dp"></TextView>
            <TextView android:layout_marginLeft="35dp" android:textColor="@android:color/black"  android:id="@+id/textView4" android:text="Groups" android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="0dp"></TextView>
        </TableRow>
    </TableLayout>
    <TableLayout android:layout_width="match_parent" android:id="@+id/tableLayout4" android:layout_height="wrap_content">
        <TableRow android:id="@+id/tableRow12" android:layout_width="wrap_content" android:layout_height="wrap_content"></TableRow>
        <TableRow android:id="@+id/tableRow13" android:layout_width="wrap_content" android:layout_height="wrap_content"></TableRow>
        <TableRow android:id="@+id/tableRow14" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <Button android:layout_marginLeft="15dp" android:id="@+id/btnResources" android:layout_height="wrap_content" android:text="Button" android:layout_weight="1" android:layout_width="0dp"></Button>
            <Button android:layout_marginRight="15dp" android:id="@+id/btncalender" android:layout_height="wrap_content" android:text="Button" android:layout_weight="1" android:layout_width="0dp"></Button>
        </TableRow>
        <TableRow android:id="@+id/tableRow15" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:layout_marginLeft="35dp" android:textColor="@android:color/black"  android:id="@+id/textView7" android:text="Gitwork Resources" android:layout_weight="1" android:layout_height="wrap_content" android:layout_width="0dp"></TextView>
            <TextView android:layout_marginLeft="35dp" android:textColor="@android:color/black"  android:id="@+id/textView8" android:text="Calender Sync" android:layout_weight="1" android:layout_height="wrap_content" android:layout_width="0dp"></TextView>
        </TableRow>
    </TableLayout>
    
</LinearLayout>
